    "\n",
    "# Define corresponding labels\n",
    "labels = ['Positive', 'Neutral', 'Negative']\n",
    "\n",
    "# Apply conditions to create the 'sentiment' column\n",
    "df['sentiment'] = np.select(conditions, labels, default='Unknown') # default for unhandled cases if any\n",
    "\n",
    "print(\"Sentiment Analysis Results:\")\n",
    "df[['review_for_sentiment', 'sentiment_score', 'sentiment']]"
